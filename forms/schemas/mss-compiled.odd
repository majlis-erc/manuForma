    <egXML xmlns="http://www.tei-c.org/ns/Examples">The address is Southmoor 
        <choice>
            <expan>Road</expan>
            <abbr>Rd</abbr>
        </choice>
      </egXML>
  </exemplum>
  <exemplum versionDate="2008-04-06" xml:lang="fr">
