   <binding ident="binding"
            elementName="binding"
            elementLabel="*Binding Description Container (2/2)"
            minOccur="0"
            maxOccur="50"
            errorMessage=""
            path="/*:binding"
            currentLevel="1"
            popup="1"
            teiElement="&lt;binding/&gt;">
      <desc>Description of the material, dimensions, and preservation
                            of the binding. Specify in the attribute if the binding is contemporary to the manuscript's production or not with the
                            values "true" or "false".</desc>
      <availableAtts elementName="binding"/>
      <childElements>
         <child>
            <element ident="decoNote"
                     label="Detailed description of binding"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="condition"
                     label="State of the binding"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
      </childElements>
   </binding>
   <decoNote ident="decoNote"
             elementName="decoNote"
             elementLabel="Detailed description of binding"
             minOccur="0"
             maxOccur="50"
             errorMessage=""
             path="/*:binding/*:decoNote"
             currentLevel="2"
             popup="2"
             teiElement="&lt;decoNote/&gt;">
      <desc>Describe the binding according to its type, material,
                            origins, and dimensions.</desc>
      <availableAtts elementName="decoNote"/>
      <childElements>
         <child>
            <element ident="objectType"
                     label="Binding type"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="material"
                     label="Binding material"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="origDate"
                     label="Date of origin"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="origPlace"
                     label="Place of origin"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="dimensions"
                     label="*Binding dimensions"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
            <element ident="note"
                     label="Note"
                     minOccurs=""
                     maxOccurs=""
                     errorMessage=""/>
         </child>
      </childElements>
   </decoNote>
   <objectType ident="objectType"
               elementName="objectType"
               elementLabel="Binding type"
               minOccur="0"
               maxOccur="50"
               errorMessage=""
               path="/*:binding/*:decoNote/*:objectType"
               currentLevel="3"
               popup="3"
               teiElement="&lt;objectType/&gt;">
      <desc>Choose whether the binding type is original, had been added after acquisition, or is absent.</desc>
      <availableAtts elementName="objectType"/>
      <controlledValues>
         <element xmlns="" ident="objectType">
            <valList type="closed">
               <valItem ident="Original"/>
               <valItem ident="Later"/>
               <valItem ident="Unbound"/>
            </valList>
         </element>
      </controlledValues>
      <childElements>
         <child/>
      </childElements>
   </objectType>
